generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model review {
  review_id Int       @id @default(autoincrement())
  comment   String?   @db.VarChar(200)
  star      Int?
  createAt  DateTime? @default(now()) @db.Timestamp(0)
}
